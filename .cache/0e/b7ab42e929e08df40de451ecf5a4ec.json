n    }\n  }\n  updateArtistQuery = (event) => {\n    this.setState({ artistQuery: event.target.value });\n  }\n\n  onKeyPress = (event) => {\n    if (event.key === 'Enter') {\n      this.searchArtist();\n    }\n  }\n\n  searchArtist = () => {\n    const API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\n    fetch(`${API_ADDRESS}/artist/${this.state.artistQuery}`)\n      .then((response) => response.json())\n      .then((responseData) => {\n        if (responseData.artists.total > 0) {\n          const artist = responseData.artists.items[0];\n          this.setState({ artist });\n          console.log(this.state.artist);\n          fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n            .then((response) => response.json())\n            .then((responseData) => {\n              this.setState({ tracks: responseData.tracks })\n              console.log(this.state.tracks);\n            })\n            .catch(error => alert(error.message));\n        }\n      })\n      .catch(error => alert(error.message));\n  }\n\n  render() {\n    return (\n      <div>\n        <h2>Music Master</h2>\n        <input placeholder='Search for an Artist' onChange={this.updateArtistQuery} onKeyPress={this.handleKeyPress} />\n        <button onClick={this.searchArtist} >Search</button>\n        <hr />\n        <Artist artist={this.state.artist} />\n      </div>\n    )\n  }\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"dfa2b47aa02fa25f730d2bfc49b5a9f7","cacheData":{"env":{}}}